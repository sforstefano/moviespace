
 <div class="container">
 	<% if flash.present? %>
	<div class="row">
		<div class="col-sm-12 col-md-6 colmd-offset-3" style="padding:25px">
		<%= render partial: "shared/flash_messages", flash: flash %>
	</div>
	<% end %>	
	 <div class="show"> 
		 <div class="col-lg-6">
			 <div class="showPeli">
				 <%if !@setting.nil?%>
				 
				   	 <p>País: <%= @setting.country %></p>
				   	 <br/>
				   	 <p>Idioma: <%= @setting.language %></p>
				   	 <br/>
				 
				     <p><%= link_to ' Editar', edit_setting_path(@setting.id) %></p>
				    
				 <%else%>
				 
				   	<%= link_to 'Añadir', new_setting_path %>
				 <%end%>
			 </div>
		 </div>
	 </div>
	 		<% if !@payment.nil? %>
				<h2>Payments</h2>
				<%@payment.each do |payment|%>
					<p><b>Subscription active:</b></p>
					<p><%= payment.subscription %></p>
					<p><b>Payment Channel:</b></p>
					<p><%= payment.channel %></p>
					<p><b>Payment active:</b></p>
					<p><%= payment.active %></p>
					<p><b>Plan:</b></p>
					<p><%= payment.plan %></p> 
					<p><b>Amount payed:</b></p>
					<p><%= payment.amount %></p>
					<hr>				
				<%end%>
			<%end%>

 </div>
